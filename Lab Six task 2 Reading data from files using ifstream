#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main() {
    ifstream inFile("LabSixTask1.txt");

    if (!inFile.is_open()) {
        cerr << "Could not open file " << endl;
        return 1;
    }

    string name;
    int age;
    float height;

    while (inFile >> name >> age >> height) {
        cout << "Name " << name
        << "Age " << age
        << "Height " << height << "cm" << endl;


    }

    inFile.close();

    return 0;
    
}
